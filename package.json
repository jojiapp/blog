{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jojiapp.github.io/blog/",
  "scripts": {
    "dev": "next dev",
    "rmdir": "rm -rf .next && rm -rf out",
    "prebuild": "yarn rmdir",
    "build": "next build",
    "prestart": "yarn build",
    "start": "next start",
    "preexport": "yarn build",
    "export": "next export && touch out/.nojekyll",
    "rmgh": "git push origin --delete gh-pages",
    "push": "git add . && git ci -m deploy && git push origin master",
    "predeploy": "yarn export && yarn push",
    "deploy": "git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "next": "10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-markdown": "^5.0.2",
    "react-syntax-highlighter": "^15.3.0",
    "remark-gfm": "^1.0.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@zeit/next-sass": "^1.0.1",
    "node-sass": "4.14.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1"
  }
}
